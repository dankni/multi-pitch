{
  "name": "multi-pitch",
  "version": "1.0.1",
  "description": "A site dedicated to multi-pitch rock climbing",
  "main": "generateCardClimb.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "generate-climbs": "node generateCardClimb.js",
